<?xml version='1.0' encoding='utf-8'?>
<widget id="com.grasp.beta" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>Grasp</name>
  <description>
    Knowledge is Power
  </description>
  <author email="asafsloook@gmail.com">
    Grasp Team
  </author>
  <content src="index.html" />

  <icon src="icon.png" />

  <preference name="phonegap-version" value="cli-7.1.0"/>

  <access origin="whatsapp:*" launch-external="yes" />
  <allow-navigation href="*" />
  
  <plugin name="cordova-plugin-inappbrowser" spec="2.0.2" source="npm"/>
  <plugin name="cordova-plugin-device" spec="2.0.1" source="npm"/>
  <plugin name="cordova-plugin-geolocation" spec="4.0.1" source="npm"/>
  <plugin name="cordova-plugin-console" source="npm" spec="~1.0.3" />
  <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.1" />
  <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
  <plugin name="cordova-plugin-media" source="npm" spec="~2.2.0" />
  <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.3" />
  <plugin name="cordova-plugin-x-socialsharing" spec="5.4.0" source="npm" />
  <gap:plugin name="cordova-plugin-customurlscheme" source="npm" spec="4.3.0">
    <param name="URL_SCHEME" value="grasp" />
  </gap:plugin>


  <plugin name="cordova-android-play-services-gradle-release" spec="^1.4.0" source="npm"/>
  
  <widget>
    <plugin name="cordova-plugin-googlemaps" spec="^2.0.0">
      <variable name="API_KEY_FOR_ANDROID" value="AIzaSyCzxNzo8N6KHRkVreIVvJeqnLVE2vDjJ8c" />
    </plugin>
  </widget>

  
  <plugin name="phonegap-plugin-push" source="npm" spec="1.8.0">
    <variable name="SENDER_ID" value="198839105363" />
    <variable name="FCM_VERSION" value="15.0.0" />
  </plugin>

  <platform name="android">
    <preference name="android-minSdkVersion" value="19"/>
  </platform>
  
</widget>